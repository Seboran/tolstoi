  "dependencies": {
    "lib-musique": "workspace:*",
    "lib-style": "workspace:*",
    "lib-sudoku": "workspace:*",
    "opensheetmusicdisplay": "1.9.0",
    "vue": "3.5.16"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.15.31",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "happy-dom": "17.1.2",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.5",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.2.3",
    "vue-tsc": "2.2.10"
