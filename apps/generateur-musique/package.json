  "dependencies": {
    "lib-musique": "workspace:*",
    "lib-style": "workspace:*",
    "lib-sudoku": "workspace:*",
    "opensheetmusicdisplay": "1.9.2",
    "vue": "3.5.21"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.16.3",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "autoprefixer": "10.4.21",
    "happy-dom": "17.1.2",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite": "7.1.4",
    "vitest": "3.2.4",
    "vue-tsc": "3.0.6"
  }
