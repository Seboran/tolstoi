  "devDependencies": {
    "@tsconfig/node24": "24.0.4",
    "@types/jsdom": "27.0.0",
    "@types/node": "22.19.9",
    "@vitejs/plugin-vue": "6.0.3",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "autoprefixer": "10.4.24",
    "happy-dom": "17.1.2",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3",
    "vite": "7.3.1",
