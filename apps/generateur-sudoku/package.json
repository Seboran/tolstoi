    "@testing-library/vue": "8.1.0",
    "@tsconfig/node18": "18.2.4",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.13.8",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "autoprefixer": "10.4.21",
    "jsdom": "26.0.0",
    "npm-run-all2": "7.0.2",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite": "6.3.4",
